

    <h5 style="text-align: center;" >Here are the Events your created</h5>

    <nav class="navbar navbar-light bg-light">
        <a href="/events/add"><button type="button" class="btn btn-info">Create an Event</button></a>
        <a href="/users/settings"><button type="button" class="btn btn-info">Account Setting</button></a>
      </nav>

    
    
        <table class="table table-striped">
            <thead>
              <tr>
                <th scope="col">Event</th>
                <th scope="col">Time</th>
                <th scope="col">Details</th>
                <th scope="col">Action</th>
              </tr>
            </thead>
            <tbody>
                <% for (let i in data.Events){%>
              <tr>
                    <td><%=data.Events[i].event_name%></td>
                    <td><%=`${data.Events[i].event_date.toDateString()} ${data.Events[i].formatAMPM()}`%></td>


                    <td><div class="btn-group" role="group" aria-label="Basic example">
                        <a href=<%=`/events/${data.Events[i].id}/info`%>><button type="button" class="btn btn-secondary">Info</button></a>
                      </div>
                    </td>

                    <td>
                        <div class="btn-group" role="group" aria-label="Basic example">
                          <a href=<%=`/events/${data.Events[i].id}/edit`%>><button type="button" class="btn btn-secondary">Edit</button></a>
                          &nbsp;
                          <a href=<%=`/events/${data.Events[i].id}/delete`%>><button type="button" class="btn btn-secondary"
                              onclick="return confirm('Are you sure?')">Delete</button></a> &nbsp;
                        </div>
                      </td>
                    </td>
              </tr>
              <%}%>
            </tbody>
          </table>