<% if (error){%>
    <%if(typeof error == "string"){%>
        <div class="alert alert-warning" role="alert">
            <%= error %>
        </div>
        <%}else{%>
            <% for(let i in error){%>
                <div class="alert alert-warning" role="alert">
                    <%= error[i] %>
                </div>
    
            <%}%>
        <%}%>
<% }%>